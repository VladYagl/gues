<script>
    <% include google.js %>
    let score = 0;
    let videos = [];
    let notUsed = [];

    async function play() {
        // let videos = ['u3K6_89Ee4U', '4cucHc2vef4', 'InwwkaoUBeg'];

        let videoId = notUsed[Math.floor(Math.random() * notUsed.length)];
        notUsed = notUsed.filter(id => id !== videoId);
        // let videoId = 'AZKPd3k6O6A'

        let threads = await get_threads(videoId)
        let video = await get_video(videoId);
        let channel = get_channel(video)

        console.log(video)

        let author = ""
        let comment = ""

        for (let i = 0; i < 20; i++) {
            author = get_author(threads[i])
            comment = get_comment(threads[i])

            if (author !== channel) {
                break
            }
        }

        let newc = comment.split(/(?=[\s,'.\n\r\t]+)/)
        newc = newc.map(word => {
            let clean = word.toLowerCase().trim().replace(/[,'.\n\r\t\s]/, '')
            if (/^[a-zA-Z0-9()]+$/.test(clean) && clean.length > 2 && channel.toLowerCase().includes(clean)) {
                return "_______"
            } else {
                return word
            }
        })
        comment = newc.join("")

        let table = $('#table')
        $("#status").html("")
        $('#score').css('display', 'inline-block').text(score)
        $('.hide-start').hide()
        $('#iamgenius').show()
        $('#rules').text("Guess the music video by the top comment")
        table.empty().show()
        $('#likes').text(get_likes(threads[0]))
        $('#thumb').attr("src", get_profile(threads[0]))
        $('#author').text(author)
        $('#comment').text(comment)
        $('#time').text(get_time(threads[0]))

        let selected = shuffle(videos.filter(x => x !== videoId)).slice(0, 5);
        // let selected = shuffle(videos.filter(x => x !== videoId));
        selected.push(videoId)
        selected = shuffle(selected)

        let sel = []
        for (const videoId of selected) {
            sel.push(await get_video(videoId));
        }
        selected = sel

        for (let i = 0; i < selected.length; i += 3) {
            let row = $('<tr>').appendTo(table)
            for (const video of selected.slice(i, i + 3)) {
                row.append($('<td>').text(get_title(video).split('-')[0].slice(0, 30)))
            }
            row = $('<tr>').appendTo(table)
            for (const video of selected.slice(i, i + 3)) {
                const test = get_title(video).split(/[-â€”](.+)/)
                if (test.length > 1) {
                    row.append($('<td>').text(test[1].slice(0, 30)))
                } else {
                    row.append($('<td>'))
                }
            }
            row = $('<tr>').appendTo(table)
            for (const video of selected.slice(i, i + 3)) {
                row.append($('<td>', {
                    class: "choice-container"
                }).append($('<img>', {
                    id: video.id,
                    class: 'choice',
                    src: get_thumbnail(video),
                    width: 300
                })));
            }
        }

        $('img.choice').click(img => {
            let selectedId = img.currentTarget.id;
            if (selectedId === videoId) {
                inc_score()
                const snackbar = $("#snackbar");
                snackbar.text("Correct").addClass("show").css("background-color", "#228822")
                setTimeout(_ => snackbar.removeClass("show"), 1500)
                play()
            } else {
                score = 0
                notUsed = videos;
                $('#play').show();
                table.hide();

                const snackbar = $("#snackbar");
                snackbar.text("Wrong").addClass("show").css("background-color", "#AA2222")
                setTimeout(_ => snackbar.removeClass("show"), 1500)
                $("#status").html(`<p style="color:#AA2222">It was: ${get_title(video)}</p>`)
            }
        })
    }

    function loadList(list) {
        loadVideos(list).then(vids => {
            videos = vids;
            notUsed = videos;
            $('#loaded').text("Selected list: " + list)
        })
    }

    async function load() {
        const list = $('#select option:selected').text()
        // console.log(list)
        loadList(list)
    }

    function inc_score() {
        score++
        $('#score').text(score);
    }

    $(document).ready(function () {
        loadClient()
        loadList('videos')
        fetch('/lists').then(res => {
            res.json().then(list => {
                list.forEach(el => {
                    $('#select').append($('<option>', {
                        value: el,
                        text: el
                    }))
                })
            })
        })
    });

</script>

